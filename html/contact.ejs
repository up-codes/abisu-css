<% let path = 'contact'; %>
<%- include('./_header', {path: path}) %>

<div class="under-main-visual">
  <div class="pic up-ofi-35">
    <img src="images/dummy.png" alt="">
  </div>
  <div class="tbox">
    <div class="ttl"><%= jsonData.pages[path].title %></div>
  </div>
</div>

<%- include('./_nav') %>
<div class="bread-box bgc01">
  <div class="inner fead-up">
    <ul>
      <li><a href="index.html">HOME</a></li>
      <li><%= jsonData.pages[path].title %></li>
    </ul>
  </div>
</div>

<!-- メインコンテンツ -->
<main class="main" id="main">
  <section class="contact01box area">
    <div class="inner fead-up">
      <h2 class="heading wave01 up-ta-c up-fz-30 up-fz-28@tb up-fz-24@sp up-fw-bold up-mb-80 up-mb-60@tb up-mb-40@sp">お問い合わせ</h2>
      <div class="up-ta-c up-fw-bold up-fz-20 up-fz-18@sp">
        メール問合せはこちら。お急ぎの方は電話でご連絡下さい。
      </div>
      <div class="border07 up-maxw-500 up-mx-auto up-my-60 up-my-50@tb up-my-30@sp">
        <div class="telbox02 tel color03 up-fz-38 up-fz-34@tb up-fz-28@sp">
          <img src="images/tel_icon.png" alt="">
          <span>00-0000-0000</span>
        </div>
      </div>
      <div class="border09 up-py-50 up-py-40@tb up-py-30@sp up-px-50 up-px-30@tb up-px-20@sp">
        <div class="ttl up-fz-20 up-fw-bold up-ta-c up-mb-50 up-mb-40@tb up-mb-30@sp">メールフォーム</div>
        <div class="contact-form">
          <dl class="up-d-flex up-fxw-wrap up-jc-sb">
            <dt>お名前<span>※必須</span></dt>
            <dd>
              <input type="text">
            </dd>
          </dl>
          <dl class="up-d-flex up-fxw-wrap up-jc-sb">
            <dt>電話番号<span>※必須</span></dt>
            <dd>
              <input type="text">
            </dd>
          </dl>
          <dl class="up-d-flex up-fxw-wrap up-jc-sb">
            <dt>メールアドレス<span>※必須</span></dt>
            <dd>
              <input type="text">
            </dd>
          </dl>
          <dl class="up-d-flex up-fxw-wrap up-jc-sb">
            <dt>ご相談内容<span>※必須</span></dt>
            <dd>
              <div class="form-shape">
                <input type="radio" name="" id="">
                <label for="">相談の項目が入ります</label>
                <input type="radio" name="" id="">
                <label for="">相談の項目が入ります</label>
                <input type="radio" name="" id="">
                <label for="">相談の項目が入ります</label>
              </div>
            </dd>
          </dl>
          <dl class="up-d-flex up-fxw-wrap up-jc-sb">
            <dt>お問い合わせ内容<span>※必須</span></dt>
            <dd>
              <textarea name="" id="" cols="30" rows="10"></textarea>
            </dd>
          </dl>
          <div class="btn-cmn02 __noarrow up-mx-auto up-mt-50 up-mt-40@tb up-mt-30@sp">
            <button>確認する</button>
          </div>
          <ul class="up-d-flex up-jc-se up-mt-50 up-mt-40@tb up-mt-30@sp btn-list">
            <li class="up-w-48%">
              <div class="btn-cmn02 __noarrow up-mx-auto">
                <button>戻る</button>
              </div>
            </li>
            <li class="up-w-48%">
              <div class="btn-cmn02 __noarrow up-mx-auto">
                <button>送信</button>
              </div>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </section>
</main>

<%- include('./_footer', {path: path}) %>