      <div class="js-mainimg">
        <div class="up-ofi-110">
          <img src="images/600x780.jpg.webp" alt="">
        </div>
      </div>
      <div class="up-d-flex up-fxw-wrap up-jc-sb up-mt-40 up-mt-30@tb up-mt-20@sp">
        <div class="up-w-48% up-w-100%@sp">
          <ul class="js-thumb up-col-3">
